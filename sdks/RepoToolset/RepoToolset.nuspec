<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>RoslynTools.RepoToolset</id>
    <summary>Common toolset for repositories</summary>
    <description>Common toolset for repositories</description>
    <version>$version$</version>
    <authors>Microsoft</authors>
    <owners>Microsoft</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <licenseUrl>$licenseUrl$</licenseUrl>
    <projectUrl>$repoUrl$</projectUrl>
    <copyright>$copyright$</copyright>
    <repository url="$repoUrl$" />
    <developmentDependency>true</developmentDependency>
    <tags>Toslyn Build Repository Toolset MSBuild SDK</tags>
    <serviceable>true</serviceable>
    <language>en-US</language>
  </metadata>
  <files>
    <file src="*.*" target="tools" />
    <file src="$ArtifactsBinDir$RoslynTools.BuildTasks\netcoreapp1.0\*.*" target="tools\tasks\netcoreapp1.0" />
    <file src="$ArtifactsBinDir$RoslynTools.BuildTasks\net46\RoslynTools.*.*" target="tools\tasks\net46" />
    <file src="$ArtifactsBinDir$RoslynTools.BuildTasks\net46\System.*.*" target="tools\tasks\net46" />
  </files>
</package>