      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/../../artifacts/bin/Microsoft.RoslynTools/Debug/net9.0/Microsoft.RoslynTools.dll",
      "args": [
        "pr-finder",
        "-s",
        "d7261529b43f4581fb47e8f4d3d9197095d44508",
        "-e",
        "main",
        "--path",
        "src/dotnet-roslyn-tools",
        "-v",
        "diag"
