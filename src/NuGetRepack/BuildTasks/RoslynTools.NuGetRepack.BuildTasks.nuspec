<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>RoslynTools.NuGetRepack.BuildTasks</id>
    <summary>Build task for updating version of NuGet packages.</summary>
    <description>Build task for updating version of NuGet packages.</description>
    <version>$version$</version>
    <authors>Microsoft</authors>
    <owners>Microsoft</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <licenseUrl>$licenseUrl$</licenseUrl>
    <projectUrl>$repoUrl$</projectUrl>
    <copyright>$copyright$</copyright>
    <repository url="$repoUrl$" />
    <developmentDependency>true</developmentDependency>
    <tags>Roslyn Build Tool</tags>
    <serviceable>true</serviceable>
    <language>en-US</language>
  </metadata>
  <files>
    <file src="netcoreapp2.0\*.*" target="tools\netcoreapp2.0" />
    <file src="net461\RoslynTools.*.*" target="tools\net461" />
    <file src="net461\NuGet.Versioning.*" target="tools\net461" />
  </files>
</package>
