<config>
  <repo owner="dotnet" name="roslyn">
    <!-- Roslyn branches (non-vs-deps) (flowing between releases) -->
    <merge from="dev15.0.x" to="dev15.5.x" />
    <merge from="dev15.7.x" to="dev15.8.x" />
    <merge from="dev15.8.x" to="dev15.9.x" />
    <merge from="dev15.9.x" to="dev16.0.x" />
    <merge from="dev16.0.x" to="master" />

    <!-- Roslyn branches (between vs-deps branches) -->
    <merge from="dev15.7.x-vs-deps" to="dev15.8.x-vs-deps" />
    <merge from="dev15.8.x-vs-deps" to="dev15.9.x-vs-dep" />
    <merge from="dev15.9.x-vs-deps" to="dev16.0.x-vs-deps" />
    <merge from="dev16.0.x-vs-deps" to="master-vs-deps" />

    <!-- Roslyn branches (from non-vs-deps to vs-deps) -->
    <merge from="dev15.7.x" to="dev15.7.x-vs-deps" />
    <merge from="dev15.8.x" to="dev15.8.x-vs-deps" />
    <merge from="dev15.9.x" to="dev15.9.x-vs-deps" />
    <merge from="dev16.0.x" to="dev16.0.x-vs-deps" />
    <merge from="master" to="master-vs-deps" />

    <!-- Roslyn feature branches -->
    <merge from="master" to="features/razorFar" />
    <merge from="master" to="features/editorconfig-in-compiler" />
    <merge from="master" to="features/compiler" />
    <merge from="master" to="features/embeddedJson" />
    <merge from="master" to="features/embeddedRegex" />
    <merge from="master" to="features/generic-attributes" />
    <merge from="master" to="features/async-streams" />
    <merge from="master" to="features/decon-default" />
    <merge from="master" to="features/caller-expression" />
    <merge from="master" to="features/target-typed-new" />
  </repo>
  <repo owner="dotnet" name="testimpact">
    <merge from="dev15.8.x" to="dev15.9.x" />
    <merge from="dev15.9.x" to="dev16.0.x" />
    <merge from="dev16.0.x" to="master" />
  </repo>
  <repo owner="dotnet" name="project-system">
    <merge from="dev15.0.x" to="dev15.8.x" />
    <merge from="dev15.8.x" to="dev15.9.x" />
    <merge from="dev15.9.x" to="dev16.0.x" />
    <merge from="dev16.0.x" to="master" />
  </repo>
  <repo owner="dotnet" name="cli">
    <merge from="release/2.0.0" to="release/2.1.1xx" />
    <merge from="release/2.1.1xx" to="release/2.1.2xx" />
    <merge from="release/2.1.2xx" to="release/2.1.3xx" />
    <merge from="release/2.1.3xx" to="release/2.1.4xx" />
