<Project>
  
  <!-- Removes files from the `*.symbols.nupkg` packages that cause errors with symbol publishing. -->
  <Target Name="IgnoreSymbolPackage" AfterTargets="Pack" BeforeTargets="BeforePublish">
    <ItemGroup>
      <ExistingSymbolPackages Include="$(ArtifactsNonShippingPackagesDir)**/Microsoft.Roslyn.*.symbols.nupkg" IsShipping="false" />
    </ItemGroup>
  </Target>

</Project>
